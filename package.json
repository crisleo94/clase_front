{
  "name": "react-parcel-app",
  "version": "0.0.1",
  "description": "Simple React + Parcel set up.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env BROWSER=brave-browser parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "test": "jest",
    "prettier": "prettier --check \"**/*.*\"",
    "prettier:write": "prettier --write \"**/*.*\"",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-do-expressions": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-function-sent": "7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "7.12.1",
    "@babel/plugin-proposal-throw-expressions": "7.12.1",
    "@babel/plugin-syntax-import-meta": "7.10.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "@babel/runtime-corejs3": "7.12.5",
    "@spothero/eslint-config": "1.9.1",
    "@spothero/prettier-config": "0.1.1",
    "@spothero/stylelint-config": "1.4.0",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "babel-jest": "26.6.3",
    "cross-env": "^7.0.3",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "jest-transform-stub": "2.0.0",
    "node-sass": "5.0.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.1.2",
    "react-hot-loader": "4.13.0",
    "sass": "1.29.0"
  },
  "dependencies": {
    "autoprefixer": "9.8.6",
    "axios": "0.21.0",
    "core-js": "3.7.0",
    "moment": "2.29.1",
    "postcss-modules": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7"
  }
}
